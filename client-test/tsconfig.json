{
  "compilerOptions": {
    "noImplicitReturns": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "isolatedModules": true
  }
}
